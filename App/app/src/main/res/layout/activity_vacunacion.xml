<?xml version="1.0" encoding="utf-8"?>
<androidx.core.widget.NestedScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Vacunacion">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/main"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">


        <ImageView
            android:id="@+id/imageView8"
            android:layout_width="731dp"
            android:layout_height="734dp"
            android:scaleType="fitXY"
            app:srcCompat="@drawable/fondoverde"
            tools:layout_editor_absoluteX="1dp"
            tools:layout_editor_absoluteY="3dp" />

        <Button
            android:id="@+id/BT_ING_DET"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginBottom="16dp"
            android:backgroundTint="#55A84E"
            android:fontFamily="serif"
            android:onClick="INGRESO"
            android:text="Ingreso"
            android:textStyle="bold"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.498"
            app:layout_constraintStart_toStartOf="parent" />

        <EditText
            android:id="@+id/TT_FECHA_REVACUNACION"
            android:layout_width="210dp"
            android:layout_height="53dp"
            android:layout_weight="1"
            android:ems="10"
            android:fontFamily="serif-monospace"
            android:inputType="date"
            android:textSize="16sp"
            android:textStyle="bold"
            app:layout_constraintBottom_toTopOf="@+id/BT_ING_DET"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" />

        <TableRow
            android:id="@+id/tableRow"
            android:layout_width="367dp"
            android:layout_height="62dp"
            app:layout_constraintBottom_toTopOf="@+id/textView22"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent">

            <LinearLayout
                android:id="@+id/linearLayout3"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginStart="24dp"
                android:layout_marginEnd="32dp"
                android:layout_marginBottom="8dp"
                android:layout_weight="1"
                android:orientation="vertical"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent">

                <TextView
                    android:id="@+id/TT_DESC"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:fontFamily="serif-monospace"
                    android:text="Descripción"
                    android:textSize="20sp"
                    android:textStyle="bold" />

                <TextView
                    android:id="@+id/TT_FRAB"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:fontFamily="serif-monospace"
                    android:text="Fabricación"
                    android:textSize="20sp"
                    android:textStyle="bold" />

            </LinearLayout>
        </TableRow>

        <EditText
            android:id="@+id/TT_LOTE"
            android:layout_width="223dp"
            android:layout_height="24dp"
            android:layout_marginBottom="8dp"
            android:layout_weight="1"
            android:ems="10"
            android:fontFamily="serif-monospace"
            android:inputType="text"
            android:textSize="18sp"
            android:textStyle="bold"
            app:layout_constraintBottom_toTopOf="@+id/tableRow"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.31"
            app:layout_constraintStart_toEndOf="@+id/textView18" />

        <TextView
            android:id="@+id/textView18"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="72dp"
            android:fontFamily="serif-monospace"
            android:text="N LOTE:"
            android:textSize="18sp"
            android:textStyle="bold"
            app:layout_constraintBottom_toBottomOf="@+id/TT_LOTE"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="@+id/TT_LOTE"
            app:layout_constraintVertical_bias="0.0" />

        <Spinner
            android:id="@+id/SPVacuna"
            android:layout_width="148dp"
            android:layout_height="39dp"
            android:layout_marginStart="16dp"
            android:layout_weight="1"
            android:background="@color/VerdeClaroMascota"
            app:layout_constraintBottom_toBottomOf="@+id/TP_VACUNA"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="@+id/TP_VACUNA"
            app:layout_constraintVertical_bias="1.0" />

        <Spinner
            android:id="@+id/TP_VACUNA"
            android:layout_width="207dp"
            android:layout_height="38dp"
            android:layout_marginEnd="28dp"
            android:layout_marginBottom="40dp"
            android:layout_weight="1"
            android:background="@color/VerdeClaroMascota"
            app:layout_constraintBottom_toTopOf="@+id/TT_LOTE"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="1.0"
            app:layout_constraintStart_toEndOf="@+id/SPVacuna" />

        <Spinner
            android:id="@+id/SPMascota"
            android:layout_width="212dp"
            android:layout_height="50dp"
            android:layout_marginBottom="40dp"
            android:layout_weight="1"
            android:background="@color/VerdeClaroMascota"
            app:layout_constraintBottom_toTopOf="@+id/TP_VACUNA"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@+id/textView4" />

        <TextView
            android:id="@+id/textView4"
            android:layout_width="145dp"
            android:layout_height="34dp"
            android:layout_marginStart="20dp"
            android:layout_weight="1"
            android:fontFamily="serif-monospace"
            android:text="Mascota"
            android:textSize="24sp"
            android:textStyle="bold"
            app:layout_constraintBottom_toBottomOf="@+id/SPMascota"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="@+id/SPMascota"
            app:layout_constraintVertical_bias="0.812" />

        <TextView
            android:id="@+id/APELLIDO_CLI"
            android:layout_width="135dp"
            android:layout_height="23dp"
            android:layout_marginEnd="236dp"
            android:layout_marginBottom="24dp"
            android:layout_weight="1"
            android:fontFamily="serif-monospace"
            android:text="Apellido"
            android:textSize="16sp"
            android:textStyle="bold"
            app:layout_constraintBottom_toTopOf="@+id/SPMascota"
            app:layout_constraintEnd_toEndOf="parent" />

        <TextView
            android:id="@+id/NOMBRE_CLI"
            android:layout_width="132dp"
            android:layout_height="22dp"
            android:layout_marginStart="184dp"
            android:layout_weight="1"
            android:fontFamily="serif-monospace"
            android:text="Nombre"
            android:textSize="16sp"
            android:textStyle="bold"
            app:layout_constraintBottom_toBottomOf="@+id/APELLIDO_CLI"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="@+id/APELLIDO_CLI"
            app:layout_constraintVertical_bias="1.0" />

        <Spinner
            android:id="@+id/SPCedulaBuscar"
            android:layout_width="126dp"
            android:layout_height="46dp"
            android:layout_marginEnd="84dp"
            android:background="@color/VerdeClaroMascota"
            app:layout_constraintBottom_toTopOf="@+id/NOMBRE_CLI"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/imageView9"
            app:layout_constraintVertical_bias="0.556" />

        <TextView
            android:id="@+id/textView23"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="48dp"
            android:fontFamily="serif-monospace"
            android:text="Seleccione Cedula"
            android:textStyle="bold"
            app:layout_constraintBottom_toTopOf="@+id/APELLIDO_CLI"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/imageView9"
            app:layout_constraintVertical_bias="0.564" />

        <ImageView
            android:id="@+id/imageView9"
            android:layout_width="166dp"
            android:layout_height="151dp"
            android:layout_marginTop="16dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.497"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/vacunada" />

        <TextView
            android:id="@+id/textView22"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="serif-monospace"
            android:text="Fecha de Reaplicacion"
            android:textSize="20sp"
            android:textStyle="bold"
            app:layout_constraintBottom_toTopOf="@+id/TT_FECHA_REVACUNACION"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" />

    </androidx.constraintlayout.widget.ConstraintLayout>

</androidx.core.widget.NestedScrollView>